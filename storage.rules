rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Photos: public can read, admin can write
    match /photos/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    
    // Logos: public can read, admin can write
    match /logos/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}

